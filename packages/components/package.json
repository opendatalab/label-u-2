{
  "name": "@label-u/components",
  "version": "0.0.2",
  "description": "Provide a complete library of annotation components",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build && cross-env NODE_ENV=developemnt rollup -m -c rollup.config.js -w",
    "start": "npm run dev",
    "dev:style": "dart-sass --watch src/index.scss dist/index.css ",
    "build:style": "dart-sass src/index.scss dist/index.css && shx cp -R ./src/assets/cssIcon/.  ./dist/assets/cssIcon",
    "build": "rimraf dist && rimraf es && cross-env NODE_ENV=production rollup -c rollup.config.js && npm run build:style",
    "build:type": "tsc --emitDeclarationOnly",
    "commit": "git-cz",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' && npm run build:type",
    "lint:fix": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "commitmsg": "commitlint -e $GIT_PARAMS"
  },
  "author": "wuhui",
  "contributors": [
    "lijingchi",
    "tanjunbao",
    "chenlu",
    "lihuaqi",
    "luoluo"
  ],
  "keywords": [
    "annotation",
    "canvas",
    "react"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/opendatalab/labelU/tree/main/packages/lb-components",
  "bugs": {
    "url": "https://github.com/opendatalab/labelU.git",
    "email": "751569801@qq.com"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@label-u/utils": "^1.2.0",
    "@label-u/annotation": "^1.8.0",
    "ahooks": "^3.4.0",
    "classnames": "^2.3.0",
    "copyfiles": "^2.4.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "react-i18next": "^11.12.0",
    "react-redux": "^7.2.3",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-dts": "^4.2.2",
    "shx": "^0.3.4"
  },
  "peerDependencies": {
    "antd": ">=4.15.0",
    "react": ">=16.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@types/lodash": "^4.14.168",
    "@types/react": "^16.13.1",
    "@types/react-redux": "^7.1.16",
    "@types/redux": "^3.6.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "antd": "^4.15.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dart-sass": "^1.25.0",
    "esbuild": "^0.11.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-alloy": "4.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "prettier": "^2.3.2",
    "react-draggable": "^4.4.5",
    "rollup": "^2.43.1",
    "rollup-plugin-esbuild": "^3.0.2",
    "typescript": "^4.2.3"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
